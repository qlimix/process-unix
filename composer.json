{
    "name": "qlimix/process-unix",
    "type": "package",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qlimix\\Process\\": "src/"
        }
    },
    "require": {
        "qlimix/process": "dev-master",
        "ext-pcntl": "*",
        "ext-posix": "*"
